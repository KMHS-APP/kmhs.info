{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "plugins": [{
      "name": "@astrojs/ts-plugin",
    }],
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@styles/*": ["src/styles/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
